{
  "name": "tron-marketplace-assignment",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "next dev",
    "ci": "rm -fr node_modules && yarn install --frozen-lockfile",
    "build": "next build ",
    "start": "next start",
    "test": "jest",
    "test:config": "jest src/__tests__/config/ --testPathIgnorePatterns",
    "integration-test": "start-server-and-test start http://localhost:3000 'cypress run'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,scss,json}\"",
    "format:check:staged": "pretty-quick --staged --check",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,scss,json}\"",
    "prepare": "husky install",
    "typechain": "typechain --out-dir src/config/abi/types --target=ethers-v5 \"src/config/abi/*.json\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
